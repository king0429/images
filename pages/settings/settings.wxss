/* pages/settings/settings.wxss */
page {
  min-height: 100%;
  background: #fdfdfd;
}
.fun_item {
  display: flex;
  height: 100rpx;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  padding: 0 20rpx;
  font-size: 30rpx;
  /* background: #fff; */
  border-top: 1px solid #f1f1f1;
  border-bottom: 1px solid #f1f1f1;
  margin-top: 30rpx;
}
.fun_item text:first-child {
  color: #454647;
}
.fun_item text:last-child {
  color: #999;
  font-size: 40rpx;
}